@model Clinique2000_Models.Models.FileDAttente

@{
    ViewData["Title"] = "Details";
}

<h1>Détails</h1>

<div>
    <h4>File d'attente du @Model.DateHeureInscriptions.Date</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateHeureInscriptions)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateHeureInscriptions)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateHeureOuverture)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateHeureOuverture)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateHeureFermeture)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateHeureFermeture)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NombreMedecins)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreMedecins)
        </dd>
    </dl>
</div>
<div>
    <a type="button" class="btn btn-outline-warning" asp-area="CliniqueAdmin" asp-action="Edit" asp-route-id="@Model?.Id">Modifier</a>
    <a type="button" class="btn btn-outline-secondary" href="javascript:history.back()">Retour</a>
</div>
